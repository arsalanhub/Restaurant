@model IEnumerable<Menu>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Menu</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Id
            </th>
            <th>
                Name
            </th>
            <th>
                Price
            </th>
            <th>
                Category
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="25%">
                    @obj.Id
                </td>
                <td width="25%">
                    @obj.Name
                </td>
                <td width="25%">
                    @obj.Price
                </td>
                <td width="25%">
                    @obj.Category
                </td>
            </tr>
        }
    </tbody>
</table>